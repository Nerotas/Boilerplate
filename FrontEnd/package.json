{
    "name": "erotasapp-console",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "react-scripts build",
        "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
        "freshInstall": "clear && rm -rf ./node_modules && rm -rf ./package-lock.json && npm install --package-lock-only && npm ci && npm run format",
       "start": "react-scripts start",
        "test:ci": "react-scripts test --ci --bail=10 --watchAll=false --runInBand --detectOpenHandles --testFailureExitCode --forceExit",
        "test:coverage:ci": "npm run test:ci -- --coverage --silent --verbose --coverageReporters=json-summary --runInBand --detectOpenHandles --forceExit",
        "test:coverage": "npm run test -- --coverage",
        "test": "react-scripts test --detectOpenHandles --env=jsdom --watchAll",
        "uts:coverage": "react-scripts test --env=jsdom --coverage"
    },
    "dependencies": {
        "@actions/core": "^1.10.1",
        "@analytics/google-analytics": "^0.5.3",
