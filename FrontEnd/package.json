        "typescript": "^4.9.4",
        "wait-on": "^7.0.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!**/node_modules/**"
        ],
        "coveragePathIgnorePatterns": [
            "src/assets",
            "src/testingUtils",
            "src/assets",
            "src/constants",
            "src/models"
        ],
        "coverageThreshold": {
            "global": {
                "lines": 50,
                "statements": 50
            }
        },
        "moduleNameMapper": {
            "^axios$": "axios/dist/axios.js"
        }
    }
